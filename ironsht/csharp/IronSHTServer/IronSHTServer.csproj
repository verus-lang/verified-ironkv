  </PropertyGroup>

  <PropertyGroup>
    <NoWarn>1701;1702;162;164;168;183;219;436;1717;1718</NoWarn>
  </PropertyGroup>

  <PropertyGroup>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
  </PropertyGroup>

  <PropertyGroup>
